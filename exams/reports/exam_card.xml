<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="exam_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div>
                    <div>
                        <h2>Exam Card</h2>
                        <t t-esc="doc.student_name"/>
                        <t t-esc="doc.student_class"/>
                        <t t-esc="doc.student_subject"/>
                    </div>
                </div>
            </t>
        </t>
    </template>
</odoo>