<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="student_card">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div style="margin:20px 50px;font-size:18px;">
                    <h3 style="text-align:center;">STUDENT CARD</h3>
                    <h3>Roll.No: </h3><t t-esc="doc.roll_no"/>
                    <h3>Name: </h3><t t-esc="doc.student_name"/>
                    <h3>Date of Birth: </h3><t t-esc="doc.date_of_birth"/>
                    <h3>Age: </h3><t t-esc="doc.age"/>
                </div><hr/>
            </t>
        </t>
    </template>

</odoo>