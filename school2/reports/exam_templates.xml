<odoo>

       <template id="report_session_view">
        <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="oe_structure">
                            <h1>Exam report</h1>
                            <table classs="table table-condensed">
                                <thead>
                                    <tr>
                                       <th>Name</th>
                                       <th>Class</th>
                                       <th>Subject</th>
                                       <th>Marks</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr t-foreach="docs" t-as="marks">
                                        <td><t t-esc="marks.student_id.name"></t></td>
                                        <td><t t-esc="marks.class_id.name"></t></td>
                                        <td><t t-esc="marks.subjects_ids.name"></t></td>
                                        <td><t t-esc="marks.marks"></t></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
            </t>
        </t>
    </template>

    <report
        id="report_session"
        model="school2.exam"
        string="Exams Report"
        name="school2.report_session_view"
        file="school2.report_session_view"
        report_type="qweb-pdf" />



</odoo>
