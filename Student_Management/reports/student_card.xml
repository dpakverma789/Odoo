<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="student_card">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div style="margin:20px 50px;font-size:18px;">
                    <h3 style="text-align:center;">STUDENT CARD</h3>
                    <h3>Name: </h3><t t-esc="doc.studentName"/>
                    <h3>Class: </h3><t t-esc="doc.studentClass"/>
                </div><hr/>
            </t>
        </t>
    </template>
</odoo>